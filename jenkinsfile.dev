pipeline {

    agent any

    stages {

        stage('Build') {
            steps {
                sh 'mvn clean install package'
            }
        }	
  }

slackSend channel: 'cicd-notifications', color: 'good', message: '[${currentBuild.result}] #${env.BUILD_NUMBER} ${env.BUILD_URL}', teamDomain: 'kitconcept', token: '<ADD-TOKEN-HERE>'
}
